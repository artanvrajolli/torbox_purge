
API_TOKEN_TORBOX=your_torbox_api_token
# ETA threshold in seconds (7 days = 604800 seconds)
ETA_THRESHOLD_TORBOX=604800
# Stall threshold in seconds (30 minutes = 1800 seconds)
STALL_THRESHOLD_TORBOX=1800
# Check interval in seconds (10 minutes = 600 seconds)
CHECK_INTERVAL_TORBOX=600
# Comma-separated list of file types to check: "webdl" or "torrent" or both
FILES_TYPES=torrent,webdl
